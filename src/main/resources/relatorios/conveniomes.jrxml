	<queryString>
		<![CDATA[SELECT
     COUNT(atendimento.`nome`) AS quantidade,
     atendimento.`id` AS id,
     atendimento.`nome` AS nome,
     atendimento.`hospital` AS hospital,
     atendimento.`convenio` AS convenio,
     atendimento.`medico` AS medico,
     atendimento.`data` AS data,
     atendimento.`procedimento` AS procedimento
FROM
     `atendimento` atendimento
WHERE
     (data BETWEEN $P{datainicial} AND $P{datafinal})
 AND (convenio LIKE '%$P!{convenio}%')
 AND (hospital LIKE '%$P!{hospital}%')
 AND (medico LIKE '%$P!{medico}%')
 AND (procedimento LIKE '%$P!{procedimento}%')
GROUP BY
     nome]]>
	</queryString>
